{
	"name": "ultimate-nest",
	"version": "1.6.0",
	"description": "NestJS + MikroORM realworld API example",
	"license": "MIT",
	"repository": "https://github.com/rubiin/ultimate-nest",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"lint": "eslint '{src,test}/**/*.ts' --cache --fix",
		"test": "jest",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json --runInBand --forceExit",
		"orm": "npx mikro-orm"
	},
	"dependencies": {
		"@aws-sdk/client-ses": "^3.163.0",
		"@casl/ability": "^6.1.1",
		"@golevelup/nestjs-modules": "^0.6.1",
		"@golevelup/nestjs-rabbitmq": "^3.2.0",
		"@mikro-orm/core": "^5.4.0",
		"@mikro-orm/migrations": "^5.4.0",
		"@mikro-orm/nestjs": "^5.1.1",
		"@mikro-orm/postgresql": "^5.4.0",
		"@mikro-orm/reflection": "^5.4.0",
		"@mikro-orm/sql-highlighter": "^1.0.1",
		"@nestjs/axios": "^0.1.0",
		"@nestjs/class-transformer": "^0.4.0",
		"@nestjs/class-validator": "^0.13.4",
		"@nestjs/common": "9.0.11",
		"@nestjs/config": "^2.2.0",
		"@nestjs/core": "9.0.11",
		"@nestjs/jwt": "^9.0.0",
		"@nestjs/passport": "^9.0.0",
		"@nestjs/platform-express": "^9.0.11",
		"@nestjs/serve-static": "^3.0.0",
		"@nestjs/swagger": "^6.1.2",
		"@nestjs/terminus": "^9.1.1",
		"@nestjs/throttler": "^3.0.0",
		"@ntegral/nestjs-sentry": "^3.0.7",
		"@sentry/hub": "^7.12.1",
		"@sentry/node": "^7.12.1",
		"@sentry/types": "^7.12.1",
		"@supercharge/request-ip": "^1.2.0",
		"argon2": "^0.29.1",
		"cache-manager": "^4.1.0",
		"cache-manager-redis-store": "^2.0.0",
		"cloudinary": "^1.31.0",
		"compression": "^1.7.4",
		"date-fns": "^2.29.2",
		"eta": "^1.12.3",
		"firebase-admin": "^11.0.1",
		"helmet": "^6.0.0",
		"helper-fns": "^2.5.14",
		"joi": "^17.6.0",
		"nanoid": "^3.3.4",
		"nestjs-i18n": "^9.1.8",
		"nestjs-minio": "^2.2.0",
		"nestjs-pino": "^3.1.1",
		"nodemailer": "^6.7.8",
		"passport": "0.6.0",
		"passport-facebook": "^3.0.0",
		"passport-google-oauth20": "^2.0.0",
		"passport-jwt": "4.0.0",
		"pino-http": "^8.2.0",
		"pino-pretty": "^9.1.0",
		"piscina": "^3.2.0",
		"preview-email": "^3.0.7",
		"reflect-metadata": "0.1.13",
		"rxjs": "^7.5.6",
		"sharp": "^0.31.0",
		"swagger-stats": "^0.99.2",
		"twilio": "^3.81.0"
	},
	"devDependencies": {
		"@firebase/app-compat": "0.x",
		"@firebase/app-types": "0.x",
		"@golevelup/ts-jest": "^0.3.3",
		"@mikro-orm/cli": "^5.4.0",
		"@mikro-orm/seeder": "^5.4.0",
		"@nestjs/cli": "9.1.2",
		"@nestjs/schematics": "9.0.3",
		"@nestjs/testing": "9.0.11",
		"@ryansonshine/cz-conventional-changelog": "^3.3.4",
		"@types/cache-manager": "^4.0.2",
		"@types/cache-manager-redis-store": "^2.0.1",
		"@types/compression": "^1.7.2",
		"@types/jest": "^29.0.0",
		"@types/multer": "^1.4.7",
		"@types/node": "^18.7.15",
		"@types/nodemailer": "^6.4.5",
		"@types/passport": "^1.0.10",
		"@types/passport-google-oauth20": "^2.0.11",
		"@types/passport-jwt": "^3.0.6",
		"@types/preview-email": "^2.0.1",
		"@types/puppeteer": "^5.4.6",
		"@types/qs": "6.9.7",
		"@types/sharp": "^0.30.5",
		"@types/supertest": "2.0.12",
		"@types/swagger-ui-express": "^4.1.3",
		"@typescript-eslint/eslint-plugin": "^5.36.2",
		"@typescript-eslint/parser": "^5.36.2",
		"cz-conventional-changelog": "3.3.0",
		"eslint": "^8.23.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^3.5.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-no-secrets": "^0.8.9",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-unicorn": "^43.0.2",
		"express": "^4.18.1",
		"husky": "^8.0.1",
		"jest": "29.0.2",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"sample-env": "^1.0.1",
		"supertest": "6.2.4",
		"ts-jest": "29.0.0-next.0",
		"ts-loader": "9.3.1",
		"ts-node": "10.9.1",
		"tsconfig-paths": "4.1.0",
		"typescript": "^4.8",
		"webpack": "^5.74.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"testTimeout": 30000,
		"moduleNameMapper": {
			"^@entities": "<rootDir>/entities/index",
			"^@common/(.*)$": "<rootDir>/common/$1",
			"^@modules/(.*)$": "<rootDir>/modules/$1",
			"^@lib/(.*)$": "<rootDir>/lib/$1"
		},
		"rootDir": "src",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "../coverage",
		"testEnvironment": "node",
		"coverageReporters": [
			"text",
			"html"
		]
	},
	"mikro-orm": {
		"useTsNode": true,
		"configPaths": [
			"./src/common/database/mikro-orm-cli.config.ts",
			"./dist/common/database/mikro-orm-cli.config.js"
		]
	},
	"engines": {
		"node": ">=16"
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"eslint --cache --fix",
			"prettier --write"
		],
		"*.{json,md,scss,yaml,yml}": [
			"prettier --write"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
